# Sepolia RPC URL (e.g. Alchemy/Infura/your own node)
SEPOLIA_RPC_URL=

# Private key of the deployer account (NO 0x prefix).
# Keep this secret; never commit a real key.
DEPLOYER_PRIVATE_KEY=

# Optional: customize token metadata for deployment
TOKEN_NAME=OpenWallet Test Token
TOKEN_SYMBOL=OWTT
TOKEN_DECIMALS=18

# Initial supply in whole tokens (will be multiplied by 10^decimals)
INITIAL_SUPPLY_TOKENS=1000000
